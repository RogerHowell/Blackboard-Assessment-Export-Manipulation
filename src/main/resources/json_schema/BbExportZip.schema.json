{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/product.schema.json",
  "title": "Blackboard export zip file",
  "description": "A representation of the exported zip file containing assignment submissions.",
  "type": "object",
  "properties": {
    "path": {
      "type": "string"
    },
    "file_exists": {
      "type": "boolean"
    },
    "is_file_existence_checked": {
      "type": "boolean"
    },
    "module_name": {
      "type": "string"
    },
    "cohort_year": {
      "type": "string"
    },
    "task_name": {
      "type": "string"
    },
    "export_timestamp": {
      "type": "string"
    }
  },
  "required": [
    "path",
    "file_exists",
    "is_file_existence_checked",
    "module_name",
    "cohort_year",
    "task_name",
    "export_timestamp"
  ]
}
